<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.company.project.dao.dict.DictMapper">
    <resultMap id="BaseResultMap" type="com.company.project.model.dict.Dict">
        <id column="id" jdbcType="INTEGER" property="id" />
        <result column="parent_code" jdbcType="VARCHAR" property="parentCode" />
        <result column="code" jdbcType="VARCHAR" property="code" />
        <result column="value" jdbcType="VARCHAR" property="value" />
        <result column="create_date" jdbcType="DATE" property="createDate" />
        <result column="create_by" jdbcType="VARCHAR" property="createBy" />
        <result column="update_date" jdbcType="DATE" property="updateDate" />
        <result column="update_by" jdbcType="VARCHAR" property="updateBy" />
        <result column="is_delete" jdbcType="VARCHAR" property="isDelete" />
    </resultMap>

    <insert id="addDict" keyProperty="id" useGeneratedKeys="true" parameterType="dict">
        insert into t_dict values (#{id},#{parentCode},#{code},#{value},sysdate(),#{createBy}
        ,sysdate(),#{updateBy},'0')
    </insert>
</mapper>